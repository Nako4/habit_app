<div class="contents row">
  <div class="container">
    <%= form_with(model: @article_tag,url: articles_path, local: true) do |f| %>
    <%# <%= render 'error_messages', model: @article_tag %>
      <%= f.text_field :title, placeholder: "タイトル" %>
      <%= f.text_field :tag_name, placeholder: "タグ(複数つけるには','で区切ってください" %>
      <%= f.text_area :output, placeholder: "学んだ内容", size: "10x10"%>
      <%= f.text_field :action, placeholder: "行動習慣にすること" %>
      <%= f.submit "投稿する！" %>
    <% end %>
  </div>
</div>